import java.util.*;
public class Main {public static void main(String [] args){
    Scanner sc=new Scanner(System.in);
    int t=sc.nextInt();
    int count=0;
    for(int n=2;n<t;n++){
        if(t%n==0){
            count++;
        }
    }if(count==0){
        System.out.println("number is prime");
    }else{
        System.out.println("number is not prime");
    }
}
}
